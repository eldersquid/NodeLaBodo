            <div class="col-lg-6">
                <div class="contact-form">
                    <form id="contact" action="createReservation" method="post" name="contact">
                        <div class="row">
                            <div class="col-lg-12">
                                <h4>Table Reservation</h4>
                            </div>
                            <div class="col-lg-6 col-sm-12">
                                <fieldset>
                                    <input name="cust_fname" type="text" id="cust_fname" placeholder="First Name*"
                                        required />
                                </fieldset>
                            </div>
                            <div class="col-lg-6 col-sm-12">
                                <fieldset>
                                    <input name="cust_lname" type="text" id="cust_lname" placeholder="Last Name*"
                                        required />
                                </fieldset>
                            </div>
                            <div class="col-lg-6 col-sm-12">
                                <fieldset>
                                    <input name="cust_email" type="text" id="cust_email" pattern="[^ @]*@[^ @]*"
                                        placeholder="Your Email Address*" required />
                                </fieldset>
                            </div>
                            <div class="col-lg-6 col-sm-12">
                                <fieldset>
                                    <input name="cust_phone" type="text" id="cust_phone" placeholder="Phone Number*"
                                        required />
                                </fieldset>
                            </div>
                            <div class="col-md-6 col-sm-12">
                                <fieldset>
                                    <input name="number_guest" type="text" id="number_guest" placeholder="No Of Guests*"
                                        value="{{reservation.number_guest}}" required />
                                </fieldset>
                            </div>
                            <div class="col-lg-6">
                                <fieldset>
                                    <input name="cust_date" type="date2" id="cust_date" class="far fa-calendar"
                                        value="{{reservation.cust_date}}" placeholder="Date*" required />
                                </fieldset>
                            </div>
                            <div class="col-md-6 col-sm-12">
                                <fieldset>
                                    <input name="cust_time" type="time" id="cust_time" placeholder="Time*"
                                        value="{{reservation.cust_time}}" required />
                                </fieldset>
                            </div>
                            <div class="col-lg-12">
                                <fieldset>
                                    <textarea name="cust_message" rows="6" id="cust_message"
                                        placeholder="Message"></textarea>
                                </fieldset>
                            </div>
                            <div class="col-lg-12">
                                <fieldset>
                                    <button type="button" "btn btn-primary btn-sm" data-toggle="modal"
                                        data-target="#exampleModal" id="modalsubmit">
                                        Make A Reservation
                                    </button>
                                
                                    <!-- Modal -->
                                    <div class="modal fade" id="exampleModal" tabindex="-1"
                                        aria-labelledby="exampleModalLabel" aria-hidden="true">

                                        <div class="modal-dialog">
                                            <div class="modal-content">
                                                <div class="modal-header">
                                                    <h3 class="modal-title" id="exampleModalLabel"
                                                        style="float: left; width: auto; text-align: center;">
                                                        Confirmation
                                                    </h3>
                                                    <button type="button" class="close" data-dismiss="modal"
                                                        aria-label="Close" style="float: right; width: auto;">
                                                        <span aria-hidden="true">
                                                            Ã—
                                                        </span>
                                                    </button>
                                                </div>
                                                <div class="modal-body">
                                                    <h6 id="modal_body"></h6>
                                                </div>
                                                <div class="modal-footer">
                                                    <button type="submit" onclick="" class="btn btn-success btn-sm"
                                                        data-toggle="modal" data-target="#exampleModal">
                                                        Submit
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div> 
                                </fieldset>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
    </div>
</section>
