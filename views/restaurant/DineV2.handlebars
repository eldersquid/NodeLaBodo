            Swal.fire({
                title: 'Error!',
                text: 'First Name is Required!',
                icon: 'error',
                confirmButtonText: 'ok'
            })
        }
        if (!lnameInput.value) {
            isFormValid = false;
            Swal.fire({
                title: 'Error!',
                text: 'Last Name Field is Required!',
                icon: 'error',
                confirmButtonText: 'ok'
            })
        }

        if (!isValidEmail(emailInput.value)) {
