    const phoneInput = document.querySelector('input[name="cust_phone"]');
    const guestInput = document.querySelector('input[name="number_guest"]');
    const dateInput = document.querySelector('input[name="cust_date"]');
    const timeInput = document.querySelector('input[name="cust_time"]');


    const inputs = [fnameInput, lnameInput, emailInput, phoneInput, guestInput, dateInput, timeInput];

