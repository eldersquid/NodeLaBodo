                title: 'Error!',
                text: 'First Name Field is Required!',
                icon: 'error',
                confirmButtonText: 'ok'
            })
            isFormValid = false;
        }

        if (!isValidPhone(phoneInput.value)) {
            Swal.fire({
                title: 'Error!',
                text: 'First Name is Required!',
                icon: 'error',
                confirmButtonText: 'ok'
            })
            isFormValid = false;
        }

        if (!guestInput(guestInput.value)) {
            Swal.fire({
                title: 'Error!',
                text: 'First Name Field is Required!',
                icon: 'error',
                confirmButtonText: 'ok'
            })
            isFormValid = false;
        }

        if (!dateInput(dateInput.value)) {
            Swal.fire({
                title: 'Error!',
                text: 'First Name Field is Required!',
                icon: 'error',
                confirmButtonText: 'ok'
            })
            isFormValid = false;
        }

        if (!timeInput(timeInput.value)) {
            Swal.fire({
                title: 'Error!',
                text: 'First Name Field is Required!',
                icon: 'error',
                confirmButtonText: 'ok'
            })
            isFormValid = false;
        }
    };

    form.addEventListener("submit", (e) => {
        e.preventDefault();
        isValidationOn = true;
        validateInputs();
        if (isFormValid) {
            Swal.fire(
                'Confirmed!',
                'Table Reserved!',
                'success'
            )
        }
    });

    inputFields.forEach((input) => input.addEventListener("input", validateInputs));
</script> --}}